        <script src="{{ asset('jquery-3.5.1.js') }}"></script>
        <script src="{{ asset('bootstrap-4.5.0/js/bootstrap.bundle.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
